//
// AE Inventory Quality Score
//

option optimize_for = SPEED;

package rubicon.data.ad_engine;

message InventoryQualityScore_PB {
    optional double     site_qs_coeff               = 1 [default = 1];
    optional double     geo_qs_coeff                = 2 [default = 1];
    optional double     placement_qs_coeff          = 3 [default = 1];
    optional double     cookie_density_qs_coeff     = 4 [default = 1];
    optional double     cookie_age_qs_coeff         = 5 [default = 1];
    optional double     session_depth_qs_coeff      = 6 [default = 1];
    optional double     opersys_id_qs_coeff         = 7 [default = 1];
    optional double     nuid_qs_coeff               = 8 [default = 1];
    
    optional double     site_qs                     = 21;
    optional double     geo_qs                      = 22;
    optional double     placement_qs                = 23;
    optional double     cookie_density_qs           = 24;
    optional double     session_depth_qs            = 25;
    optional double     opersys_id_qs               = 26;
    optional double     nuid_qs                     = 27;
        
    optional double     final_qs                    = 50;       // quality score value calculated from previosu attributes
}
