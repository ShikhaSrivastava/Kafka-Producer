//
// all types that are shared between AE, BE, scheduler, ...
//

option optimize_for = SPEED;

package rubicon.data.common;


//
// mime types
//
enum MimeType_E {
    UNKNOWN_MIME       = 0;
    JS_MIME            = 1;
    HTML_MIME          = 2;
    IMAGE_MIME         = 3;
    REDIRECT_MIME      = 4;
    PSA_MIME           = 5;
}

//
// ad class types
//
enum AdClassType_E {
